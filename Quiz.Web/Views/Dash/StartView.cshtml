    Your browser does not support the audio element.
</audio-->

<h3>Teams ready</h3>

<p>All the teams are ready!</p>
@{
    int i = 1;
    foreach (var team in Model.Teams) {
        <h3>Team @i: @team.Name</h3>
        i++;
    }
}